{
  "sections": {
    "main": {
      "type": "main-product",
      "blocks": {
        "vendor": {
          "type": "text",
          "disabled": true,
          "settings": {
            "text": "{{ product.vendor }}",
            "text_style": "uppercase"
          }
        },
        "title": {
          "type": "title",
          "settings": {
            "title_color": "#111111",
            "title_weight": "700",
            "title_size_mobile": 22,
            "title_size_desktop": 24
          }
        },
        "price": {
          "type": "price",
          "settings": {
            "price_color": "#7c7c7c",
            "price_weight": "700",
            "price_size_mobile": 22,
            "price_size_desktop": 24,
            "compare_color": "#7b7b7b",
            "compare_weight": "400",
            "compare_size_mobile": 20,
            "compare_size_desktop": 22,
            "show_badges": false
          }
        },
        "sku_aHdUBc": {
          "type": "sku",
          "settings": {
            "sku_title": "Style",
            "text_style": "body",
            "sku_label_color": "#3a3838",
            "sku_label_weight": "600",
            "sku_label_size_mobile": 14,
            "sku_label_size_desktop": 16,
            "sku_value_color": "#444444",
            "sku_value_weight": "500",
            "sku_value_size_mobile": 14,
            "sku_value_size_desktop": 16,
            "avail_label_color": "#111111",
            "avail_label_weight": "600",
            "avail_label_size_mobile": 14,
            "avail_label_size_desktop": 14,
            "avail_value_color": "#6eca56",
            "avail_value_weight": "600",
            "avail_value_size_mobile": 14,
            "avail_value_size_desktop": 14,
            "availability_title": "Availability",
            "availability_text_in": "In stock",
            "availability_text_out": "Out of stock",
            "sku_avail_space_mobile": 20,
            "sku_avail_space_desktop": 20
          }
        },
        "product-inventory": {
          "type": "inventory",
          "disabled": true,
          "settings": {
            "text_style": "body",
            "inventory_threshold": 10,
            "show_inventory_quantity": true
          }
        },
        "variant-picker": {
          "type": "variant_picker",
          "settings": {
            "picker_type": "image",
            "swatch_shape": "circle"
          }
        },
        "quantity-selector": {
          "type": "quantity_selector",
          "settings": {}
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_dynamic_checkout": false,
            "show_gift_card_recipient": false
          }
        },
        "share_bxBCpK": {
          "type": "share",
          "settings": {
            "share_label": "Share"
          }
        }
      },
      "block_order": [
        "vendor",
        "title",
        "price",
        "sku_aHdUBc",
        "product-inventory",
        "variant-picker",
        "quantity-selector",
        "buy_buttons",
        "share_bxBCpK"
      ],
      "settings": {
        "enable_sticky_info": true,
        "color_scheme": "scheme-1",
        "media_size": "medium",
        "constrain_to_viewport": true,
        "media_fit": "cover",
        "gallery_layout": "thumbnail_slider",
        "mobile_thumbnails": "show",
        "media_position": "left",
        "image_zoom": "none",
        "hide_variants": false,
        "enable_video_looping": true,
        "padding_top": 36,
        "padding_bottom": 36
      }
    },
    "collapsible-content": {
      "type": "collapsible-content",
      "blocks": {
        "collapsible-row-0": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Collapsible row",
            "icon": "bottle",
            "row_content": "{{ product.description }}",
            "page": ""
          }
        },
        "collapsible-row-1": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Collapsible row",
            "icon": "none",
            "row_content": "",
            "page": ""
          }
        },
        "collapsible-row-2": {
          "type": "collapsible_row",
          "settings": {
            "heading": "Collapsible row",
            "icon": "none",
            "row_content": "",
            "page": ""
          }
        }
      },
      "block_order": [
        "collapsible-row-0",
        "collapsible-row-1",
        "collapsible-row-2"
      ],
      "settings": {
        "caption": "",
        "heading": "Collapsible content",
        "heading_size": "h1",
        "heading_alignment": "center",
        "layout": "section",
        "container_color_scheme": "scheme-1",
        "color_scheme": "scheme-1",
        "open_first_collapsible_row": true,
        "image_ratio": "adapt",
        "desktop_layout": "image_second",
        "padding_top": 36,
        "padding_bottom": 36
      }
    },
    "related-products": {
      "type": "related-products",
      "settings": {
        "heading": "You may also like",
        "heading_size": "h2",
        "products_to_show": 4,
        "columns_desktop": 4,
        "columns_mobile": "2",
        "color_scheme": "scheme-1",
        "image_ratio": "square",
        "image_shape": "default",
        "show_secondary_image": true,
        "show_vendor": false,
        "show_rating": false,
        "padding_top": 36,
        "padding_bottom": 28
      }
    }
  },
  "order": [
    "main",
    "collapsible-content",
    "related-products"
  ]
}
